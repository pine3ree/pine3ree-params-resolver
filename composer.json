{
    "name": "pine3ree/pine3ree-params-resolver",
    "description": "A function/method/invokable-object parameter resolution utility class",
    "type": "library",
    "keywords": [
        "pine3ree",
        "invokable",
        "parameters",
        "resolver"
    ],
    "homepage": "https://github.com/pine3ree/pine3ree-params-resolver",
    "readme": "README.md",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "pine3ree",
            "homepage": "https://github.com/pine3ree",
            "role": "Developer"
        }
    ],
    "support": {
        "docs": "https://github.com/pine3ree/pine3ree-params-resolver/blob/master/README.md",
        "issues": "https://github.com/pine3ree/pine3ree-params-resolver/issues",
        "source": "https://github.com/pine3ree/pine3ree-params-resolver"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "prefer-stable": true,
        "minimum-stability": "dev"
    },
    "require": {
        "psr/container": "^1.1.2 || ^2.0",
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^1.1 || ^2.0",
        "phpstan/phpstan": "^1.10 || ^2.0",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5.7",
        "vimeo/psalm": "^5.0 || 6.0",
        "webimpress/coding-standard": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "pine3ree\\Container\\": "src/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "pine3ree\\test\\Container\\": "test/"
        }
    },
    "scripts": {
        "phpcs": "phpcs",
        "phpcbf": "phpcbf",
        "check-cs": "@phpcs",
        "fix-cs": "@phpcbf",
        "cs-check": "@phpcs",
        "cs-fix": "@phpcbf",
        "phpstan": "phpstan analyze -c phpstan.neon",
        "psalm": "psalm --shepherd --stats",
        "static-analysis": [
            "@phpstan",
            "@psalm"
        ],
        "phpunit": "phpunit --colors=always",
        "test": "@phpunit",
        "check": [
            "@phpcs",
            "@static-analysis",
            "@test"
        ],
        "test-coverage": "@test-coverage-clover",
        "test-coverage-clover": "phpunit --colors=always --coverage-clover clover.xml",
        "test-coverage-html": "phpunit --colors=always --coverage-html coverage/html",
        "test-coverage-xml": "phpunit --colors=always --coverage-xml coverage/xml",
        "test-coverage-all": [
            "@test-coverage-html",
            "@test-coverage-xml",
            "@test-coverage-clover"
        ],
        "php-version": "php --version"
    }
}
